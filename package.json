{
  "name": "quick-quote",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "concurrently \"npm run start:dev:front\" \"npm run start:dev:server\"",
    "start:dev:front": "ng serve --proxy-config ./proxy.local.config.json",
    "start:dev:server": "cross-env NODE_ENV=development USE_CACHE=true PORT=3000 nodemon server/server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prepare": "husky install",
    "generate-icons": "svg-generator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@ngneat/svg-icon": "^4.1.4",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "nodemailer": "^6.7.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.2.3",
    "@angular/compiler-cli": "~13.2.0",
    "@commitlint/cli": "^16.2.4",
    "@ng-web-apis/common": "^2.0.0",
    "@ngneat/svg-generator": "^4.0.0",
    "@ngneat/until-destroy": "^9.2.0",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/entity": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~3.10.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "commitlint-config-jira": "^1.6.4",
    "commitlint-plugin-jira-rules": "^1.6.4",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ngrx": "^2.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "html-to-image": "^1.9.0",
    "husky": "^7.0.4",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.4.1",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.0",
    "ngx-sharebuttons": "^10.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "typescript": "~4.5.2"
  },
  "lint-staged": {
    "*.{js,md,ts,html}": [
      "prettier --write",
      "eslint"
    ]
  },
  "svgGenerator": {
    "outputPath": "./src/app/shared/svg/icons",
    "prefix": "qq",
    "srcPath": "./src/assets/svg",
    "svgoConfig": {
      "plugins": [
        "removeDimensions",
        "cleanupAttrs"
      ]
    }
  }
}
